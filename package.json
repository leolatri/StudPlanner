{
  "name": "studplanner",
  "version": "1.0.0",
  "private": true,
  "description": "Это приложение, созданное для облегчения студенческой жизни",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build:web": "expo export --platform web",
    "build:apk": "eas build -p android --profile preview-apk",
    "build:apk:local": "eas build -p android --profile preview-apk --local",
    "build": "npm run build:web && npm run build:apk"
  },
  "dependencies": {
    "expo": "~54.0.32",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-web": "0.21.0",
    "react-native": "0.81.0",
    "@expo-google-fonts/montserrat": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2",
    "eas-cli": "^16.0.0"
  }
}